container_commands:
  01_migrate:
    command: "python manage.py migrate"
    leader_only: true
  02_chown_sqlitedb:
    command: "sudo chown wsgi db.slite3"
    leader_only: true
  03_seed:
    command: "python manage.py loaddata users.json"
    leader_only: true
  04_collectstatic:
    command: "python manage.py collectstatic"
    leader_only: true

options_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: pjtback.settings
